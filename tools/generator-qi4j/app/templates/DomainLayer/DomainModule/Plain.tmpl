package <%= qi4j.packageName %>.model.<%= qi4j.current.name %>;
<%
for( var imp in qi4j.current.imported ) {
    if( !imp.startsWith( "java.lang" )
        && imp.includes(".") ) {
        %><%- "import " + imp + ";"; %>
<%
    }
} %>

public <%- qi4j.current.clazz.type %> <%= qi4j.current.clazz.name %>
{<%
if( qi4j.current.clazz.type === 'enum' ) {
%>
    <%- qi4j.current.clazz.values %>
<%
}
%>}
