dependencies {

<% if( hasFeature( 'security' ) ) {
%>    implementation "org.qi4j.libraries:org.qi4j.library.shiro-core:$qi4jVersion"
<% } %>
<% if( qi4j.applicationtype === 'Rest API' ) {
%>    implementation "org.qi4j.libraries:org.qi4j.library.restlet:$qi4jVersion"
<% } %>
<% if( qi4j.hasFeature( 'mixin scripting' ) ) {
%>    implementation "org.qi4j.libraries:org.qi4j.library.scripting:$qi4jVersion"
<% } %>
<% if( qi4j.hasFeature( 'jmx' ) ) {
%>    implementation "org.qi4j.libraries:org.qi4j.library.jmx:$qi4jVersion"
<% } %>

}
